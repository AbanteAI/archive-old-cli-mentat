{
  "name": "mentat",
  "displayName": "Mentat",
  "description": "Mentat - The AI Coding Assistant",
  "version": "0.0.1",
  "publisher": "AbanteAI",
  "repository": "https://github.com/AbanteAI/mentat/mentat-vscode",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./build/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mentat",
          "title": "Mentat",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "mentat": [
        {
          "id": "chat",
          "name": ""
        }
      ]
    },
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.startChat",
        "title": "Staring Chat session"
      }
    ]
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "compile": "tsc",
    "watch": "tsc -watch",
    "code": "code --extensionDevelopmentPath=$PWD .",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/jest": "29.5.5",
    "@types/node": "20.8.3",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "@types/vscode": "1.83.0",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "eslint": "8.51.0",
    "eslint-plugin-react": "7.33.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2"
  }
}
